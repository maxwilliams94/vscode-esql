{
  "name": "vscode-esql",
  "displayName": "ESQL Language Support",
  "description": "Syntax highlighting for ESQL",
  "version": "0.0.1",
  "publisher": "user",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxwilliams94/vscode-esql"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "esql",
      "aliases": ["ESQL", "esql"],
      "extensions": [".esql"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "esql",
      "scopeName": "source.esql",
      "path": "./syntaxes/esql.tmLanguage.json",
      "format": "json"
    }]
  }
}
